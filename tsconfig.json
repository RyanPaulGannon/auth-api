{
  "compilerOptions": {
    "target": "es6",
    "lib": ["esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "rootDirs": ["./**/*"],
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
    "database",
    "prisma",
    "package.json",
    ".github/database",
    ".github/app.ts",
    "app.ts"
  ]
}
